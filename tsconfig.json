{
  "compilerOptions": {
    "target": "es2016",
    "module": "ES2020",
    "rootDir": "./",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"],
    },
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
  },
  "include": ["./**/*.ts"]
}
